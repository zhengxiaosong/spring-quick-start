package rules.quickstart

import org.example.spring.drools.quickstart.pojo.Person;

rule "quickstart-001"
    when
        eval(true);
    then
        System.out.println("Hello world");
end

rule "quickstart-002"
    when
        $p:Person(name=="张三", age==30)
    then
        System.out.println("他确实叫张三，有30岁了");
end